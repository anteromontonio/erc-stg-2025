@Article{Hartley2006_SimplerTestsSemisparse,
  author   = {Hartley, Michael I.},
  journal  = {Annals of Combinatorics},
  title    = {Simpler {Tests} for {Semisparse} {Subgroups}},
  year     = {2006},
  issn     = {0219-3094},
  month    = dec,
  number   = {3},
  pages    = {343--352},
  volume   = {10},
  abstract = {The main results of this article facilitate the search for quotients of regular abstract polytopes. A common approach in the study of abstract polytopes is to construct polytopes with specified facets and vertex figures. Any nonregular polytope \$\$ {\textbackslash}mathcal\{Q\} \$\$may be constructed as a quotient of a regular polytope \$\$ {\textbackslash}mathcal\{P\} \$\$by a (so-called) semisparse subgroup of its automorphism group W (which will be a string C-group). It becomes important, therefore, to be able to identify whether or not a given subgroup N of a string C-group W is semisparse. This article proves a number of properties of semisparse subgroups. These properties may be used to test for semisparseness in a way which is computationally more efficient than previous methods. The methods are used to find an example of a section regular polytope of type \{6, 3, 3\} whose facets are Klein bottles.},
  doi      = {10.1007/s00026-006-0292-8},
  file     = {:Hartley_2006_SimplerTestsSemisparse.pdf:PDF},
  keywords = {51M20, 52B15, 05E25, abstract polytope, quotient polytope, semisparse subgroup, klein bottle},
  language = {en},
  url      = {https://doi.org/10.1007/s00026-006-0292-8},
  urldate  = {2023-04-21},
}

