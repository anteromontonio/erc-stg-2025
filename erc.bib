@Article{HubarMocha2022_AutomorphismGroups3,
  author   = {Hubard, Isabel and Mochán, Elías},
  journal  = {Journal of Algebraic Combinatorics},
  title    = {Automorphism groups of 3-orbit polyhedra},
  year     = {2022},
  issn     = {1572-9192},
  month    = apr,
  abstract = {A 3-orbit polyhedron is one in which the automorphism group has exactly 3 orbits on the set of incident triples (vertex, edge, face). Examples of such polyhedra are the prisms over regular n-gons as well as 5 of the Archimedean Solids. There are three classes of 3-orbit polyhedra, depending of the local configuration of their flags. In this paper, for each of the three classes, we find necessary and sufficient conditions on a set of generators of a group \$\${\textbackslash}Gamma \$\$so that there exists a polyhedron \$\$\{{\textbackslash}mathcal \{P\}\}({\textbackslash}Gamma )\$\$such that \$\${\textbackslash}Gamma \$\$acts by automorphisms with 3-flag orbits, in the given class. The techniques used to do this combine the ideas of coset geometries used to construct regular and chiral polytopes, together with voltage assignments used on the so-called symmetry type graph of the constructed polyhedra. These twists to the classical techniques shed light on a possible way to generalize our results to polytopes of any rank and any number of orbits on flags (in fact, to polytopes of any possible symmetry type graph). Finally, we use the results obtained in the paper to show that, for any given class, almost all symmetric groups are the automorphism group of a 3-orbit polyhedron in that class.},
  doi      = {10.1007/s10801-022-01127-0},
  file     = {:HubardMochan_2022_AutomorphismGroups3.pdf:PDF},
  keywords = {Polyhedra, Abstract polytopes, 3-Orbit polyhedra, Symmetric groups},
  language = {en},
  url      = {https://doi.org/10.1007/s10801-022-01127-0},
  urldate  = {2022-05-03},
}

@Article{Conder2013_SmallestRegularPolytopes,
  author   = {Conder, Marston},
  journal  = {Advances in Mathematics},
  title    = {The smallest regular polytopes of given rank},
  year     = {2013},
  issn     = {0001-8708},
  month    = mar,
  pages    = {92--110},
  volume   = {236},
  abstract = {An abstract polytope is called regular if its automorphism group has a single orbit on flags (maximal chains). In this paper, the regular n-polytopes with the smallest number of flags are found, for every rank n{\textgreater}1. With a few small exceptions, the smallest regular n-polytopes come from a family of ‘tight’ polytopes with 2⋅4n−1 flags, one for each n, with Schläfli symbol \{4∣4∣⋯∣4\}. Also with few exceptions, these have both the smallest number of elements, and the smallest number of edges in their Hasse diagram.},
  doi      = {10.1016/j.aim.2012.12.015},
  file     = {:Conder_2013_SmallestRegularPolytopes.pdf:PDF},
  keywords = {Regular polytope, Abstract polytope, String Coxeter group},
  language = {en},
  url      = {https://www.sciencedirect.com/science/article/pii/S000187081200463X},
  urldate  = {2022-04-28},
}

